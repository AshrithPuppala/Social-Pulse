# Reddit API Credentials (from https://www.reddit.com/prefs/apps)
REDDIT_CLIENT_ID=your_client_id_here
REDDIT_CLIENT_SECRET=your_client_secret_here
REDDIT_USER_AGENT=SocialPulse/1.0

# Twitter API Credentials (from https://developer.twitter.com)
TWITTER_BEARER_TOKEN=your_bearer_token_here

# Instagram API via RapidAPI (from https://rapidapi.com/hub)
RAPIDAPI_KEY=your_rapidapi_key_here

# Flask Secret Key (auto-generated on Render)
FLASK_SECRET_KEY=your_secret_key_here

# Port (Render will set this automatically)
PORT=10000
